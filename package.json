{
  "name": "portfolio-admin",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run db\" \"npm run server\" \"npm run client\"",
    "db": "mongod --dbpath ./data",
    "server": "pm2 start ./bin/www --watch",
    "client": "babel-node --plugins rewire-exports start-client.js"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "morgan": "~1.7.0",
    "pm2": "^2.4.6",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-rewire-exports": "^0.2.0",
    "concurrently": "^3.1.0",
    "mongodb": "^2.2.21",
    "monk": "^3.1.3"
  }
}
